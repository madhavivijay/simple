pipeline {
  agent {label 'jslave'}
    stages {
      stage('build') {
        steps {
              git branch: 'main', url: 'https://github.com/madhavivijay/simple.git'
              sh ' mvn clean install '
        }
      }
      stage('deploy') {
          steps {
            echo "deploy"        
          }
      }  
      }
    } 
