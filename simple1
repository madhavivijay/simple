pipeline {
  agent { label 'jslave' }
    stages {
      stage('build') {
        steps {
              git branch: 'master', url: 'https://github.com/madhavivijay/simple.git'
              sh 'mvn clean package'
        }
      }
      stage('deploy') {
          steps {
            echo "deploy"        
          }
      }  
      }
    } 
